<h1>Figure Name: <%=@figure.name%></h1>
<h4>Titles:</h4>
<ul>
  <%@figure.titles.each do |title|%>
  <li><%=title.name%></li>
  <%end%><br>
</ul>
<h4>Landmarks:</h4>
<ul>
  <%@figure.landmarks.each do |landmark|%>
  <li><%=landmark.name%></li>
  <%end%>
</ul>
<a href="/figures/<%= @figure.id %>/edit">Edit</a>
