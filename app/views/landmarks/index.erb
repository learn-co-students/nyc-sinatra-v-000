<h2>All Historical Landmarks</h2>

<table>
  <tr>
    <th>Name</th>
    <th>Year Completed</th>
    <th>Edit</th>
  </tr>

  
  <% @landmarks.each do |landmark| %>
  <tr>
    <td> <a href="/landmarks/<%= landmark.id %>"><%= landmark.name %></a></td>
    <td><%= landmark.year_completed %></td>
    <td> <a href="/landmarks/<%= landmark.id %>/edit">Edit</a></td>
  </tr>
  <% end %>
  
<table>
<br>
<br>
<a class="button" href="/">Home</a>
<br>
<a class="button" href="/figures">All Figures</a>
