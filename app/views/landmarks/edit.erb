<h2>Edit Historical Landmark</h2>


<form action="/landmarks/<%=@landmark.id %>" method="POST">
  <input type="hidden" name="_method" value="patch">
  <p><h2>Name: </h2><input type="text" name="landmark[name]" id="name" value="<%=@landmark.name%>"></p>
  <p><h3>Year Completed: </h3><input type="text" name="landmark[year_completed]" id="year_completed" value="<%= @landmark.year_completed %>"></p>
  <input type="submit" name="Edit Landmark" id="Edit Landmark"></input>
</form>

<a class="button" href="/figures">Landmarks</a>
<a class="button" href="/">Home</a>
