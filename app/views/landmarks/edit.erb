<h1>Edit Your Historical Landmark</h1>
<h2><%= @landmark.name %></h2>
<h3><%= @landmark.year_completed %></h3>
<form method="POST" action="/landmarks/<%= @landmark.id %>">
  <input type="hidden" id="hidden" name="_method" value="PATCH">
  <label for="name">Name: </label>
  <input type="text" name="landmark[name]" id="name" value="<%= @landmark.name %>">
  <label for="year_completed">Year Completed: </label>
  <input type="text" name="landmark[year_completed]" id="year_completed">
  <br><br>
  <input type="submit" value="Edit Landmark">
</form>