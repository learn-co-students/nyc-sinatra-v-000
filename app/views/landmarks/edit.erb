<h1>Update Landmark</h1>
<h2><%=@landmark.name%></h2>
<h2><%=@landmark.year_completed%></h2>
<form action="/landmarks/<%="#{@landmark.id}"%>" method="POST">
  <input id="hidden" type="hidden" name="_method" value="patch"> 
  <label>Landmark Name:</label>
 
  <br></br>
 
  <input type="text" name="landmark[name]" id="name">
 
  <br></br>
 
  <label>Landmark Year:</label>
 
  <br></br>
 
  <input type="text" name="landmark[year_completed]" id="year_completed">
 
  <br></br>

    
  <input type="submit" value="Edit Landmark">
</form>