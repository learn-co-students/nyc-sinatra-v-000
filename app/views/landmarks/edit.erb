<h1>Edit Landmark</h1>

<form action="/landmarks/<%=@landmark.id%>" method="POST">
  <label>Name:</label>
  <br></br>
  <input type="text" name="name" id="landmark[name]" value="<%=@landmark.name%>">
  <br></br>
  <label>Year Completed:</label>
  <br></br>
  <input type="text" name="year_completed" id="landmark[year_completed]" value="<%=@landmark.year_completed%>">
  <br></br>

  <input type="submit" value="Edit Landmark">
</form>
