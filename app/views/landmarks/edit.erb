<h1>Edit Landmark!</h1>

<form method="POST" action="/landmarks/<%=@landmark.id%>">
  <input type="hidden" name="_method" value="PATCH"

  <label>Landmark Name:</label><br>
  <input type="text" name="landmark[name]" id="name" value="<%=@landmark.name%>"><br><br>

  <label>Year Completed:</label><br>
  <input type="integer" name="landmark[year_completed]" id="year_completed" value="<%=@landmark.year_completed%>"><br><br>

  <input type="submit" value="Edit Landmark">

</form>
