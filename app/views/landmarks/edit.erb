<h2>Edit Landmark: <%=@landmark.name%></h2>

<form action="/landmarks/<%=@landmark.id%>" method="post">
<input type="hidden" id="hidden" name="_method" value="patch">

<label>Name:</label>
<input type="text" id="name" value="<%=@landmark.name%>" name="landmark[name]"><br/>
<label>Year Completed:</label>
<input type="text" id="year_completed" value="<%=@landmark.year_completed%>" name="landmark[year_completed]"><br></br>
<input type="submit" value="Edit Landmark">
</form>
