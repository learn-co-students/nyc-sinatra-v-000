<h2>Edit <%=@landmark.name%> created <%=@landmark.year_completed%></h2>


<form method = 'POST' action = '/landmarks/<%=@landmark.id%>'>
  <input type="hidden" name="_method" value="PATCH">
  Name:<input type='text' name='landmark[name]' id='name'>
  Year:<input type='text' name='landmark[year_completed]' id='year_completed'>

  <input type='submit' value='Edit Landmark'>
</form>


<a class="button" href="/landmarks">Landmarks</a>
<a class="button" href="/">Home</a>
