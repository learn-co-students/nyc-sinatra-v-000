<form action="/landmarks/<%=@landmark.id%>" method="post">
  <input id="hidden" type="hidden" name="_method" value="patch">
  <label> Name: </label>
  <input type="text" id="name" name="landmark[name]" value="<%=@landmark.name%>"></input><br></br>
  <label> Year Completed: </label>
  <input type="text" id="year_completed" name="landmark[year_completed]" value="<%=@landmark.year_completed%>"></input><br></br>

  <input type="submit" value="Edit Landmark">
</form>
