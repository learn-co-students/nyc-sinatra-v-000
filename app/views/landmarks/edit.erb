<h1>Update this historical landmark</h1>

<form action="/landmarks/<%= @landmark.id %>" method="POST">
  <input id="hidden" type="hidden" name="_method" value="patch">
  <label>Name: </label><input type="text" name="landmark[name]" id="name" value="<%=@landmark.name %>">
  <label>Year completed: </label><input type="integer" name="landmark[year_completed]" id="year_completed" value="<%=@landmark.year_completed%>">
  <br></br>
  <input type="submit" value="Edit Landmark">
  <br></br>
</form>
