<h1>Edit this Landmark:</h1>
<%=@landmark.name%>

<form action="/landmarks/<%=@landmark.id%>" method="POST">
  <input type="hidden" name="_method" value="patch">
  <label>Name:</label><br>
  <input type="text" id="name" value="<%=@landmark.name%>" name="landmark[name]">
  <label>Year Completed:</label><br>
  <input type="text" id="year_completed" value="<%=@landmark.year_completed%>" name="landmark[year_completed]">

  <input type="submit" value="Edit Landmark">
</form>