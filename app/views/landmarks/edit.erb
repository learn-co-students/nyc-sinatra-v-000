<form method="post" action="/landmarks/<%=@landmark.id%>">
    <input type="hidden" name="_method" value="PATCH">

    <label>Name</label>
        <input type="text" name="landmark[name]" id="name" value="<%=@landmark.name%>"></input>
    <label>Year Completed</label>
        <input type="text" name="landmark[year_completed]" id="year_completed" value="<%=@landmark.year_completed.to_s%>"></input>
    


    
    <br></br>
    <p>Landmarks</p>
    <%Landmark.all.each do |landmark|%>
        <ul>
            <%=landmark.name%>
            <input type="checkbox" name="landmark[name]" value="<%=landmark.name%>"></input>
        </ul>
    <%end%>

    <br></br>
    <p>Title</p>
    <%Title.all.each do |title|%>
        <ul>
            <%=title.name%>
            <input type="checkbox" name="landmark[name]" value="<%=title.name%>"></input>
        </ul>
    <%end%>
    <br></br>
    <input type="submit" value="Edit Landmark"></input>
</form>