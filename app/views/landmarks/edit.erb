<h1>Edit Landmark</h1>
<form action="/landmarks/<%= @landmark.id %>" method="POST">
  <input id="hidden" type="hidden" name="_method" value="PATCH">
  <label>Name: <input type="text" id="name" name="landmark[name]" value="<%= @landmark.name %>"/></label>
  <label>Year Completed: <input type="text" id="year_completed" name="landmark[year_completed]" value="<%= @landmark.year_completed.to_s %>"/></label>
  <br />
  <br />
  <input type="submit" value="Edit Landmark" />
</form>
